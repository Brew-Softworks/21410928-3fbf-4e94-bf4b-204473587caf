Players=game:GetService("\080\108\097\121\101\114\115")Player=Players.LocalPlayer Brew={isReach=false;curReach="\083\112\111\111\102";reachMagnitude=Vector3.new(1,.80000001192093,4),selBox=false,selBoxColor=Color3.fromRGB(0,0,0);cWalkspeed=16,cJumppower=50,cWalking=false;CFSpeed=1.35;Autoclick=false;Spin=false}function Brew.Interpolate(F,T,t,H)return coroutine.wrap(function()local F=tick()local Y=T.CFrame while tick()-F<H do local p=tick()-F local h=p/H local I=Y:Lerp(t,h)local R=(CFrame.new(I.Position,t.Position)):lerp(I,math.sin((h*math.pi)*.5))T.CFrame=R;(game:GetService("\082\117\110\083\101\114\118\105\099\101")).Heartbeat:Wait()end T.CFrame=t end)end function Brew.WaitForChildOfClass(F,T,t,H)local Y=tick()H=H or 9000000000 while tick()-Y<H do for T,H in pairs(T)do for T,H in pairs(H:GetChildren())do if H:IsA(t)then return H end end end wait(.01)end return nil end function Brew.Spoof(F,T,t,H)local Y Y=hookmetamethod(game,"\095\095\105\110\100\101\120",function(F,p)if not checkcaller()then if F==T then local T=string.gsub(tostring(p),"\000","")if T==t then return H end end end return Y(F,p)end)end function Brew.disableConnection(t,T)for T,t in pairs(getconnections(T))do t:Disable()end end function Brew.getSword(T)return Brew:WaitForChildOfClass({Player.Character,Player.Backpack},"\084\111\111\108")end function Brew.getHitbox(T)for T,t in pairs((Brew:getSword()):GetDescendants())do if t:FindFirstChildOfClass("\084\111\117\099\104\084\114\097\110\115\109\105\116\116\101\114")then t.Massless=true return t end end end function Brew.doReach(T)Brew:getSword()Brew:disableConnection((Brew:getHitbox()):GetPropertyChangedSignal("\083\105\122\101"))Brew:Spoof(Brew:getHitbox(),"\083\105\122\101",Vector3.new(1,.80000001192093,4))Brew.isReach=true if not identifyexecutor()=="\070\108\117\120\117\115"then damageAmplification=(Brew:getHitbox()).Touched:Connect(function(T)if Brew.isReach==true and T.Parent:FindFirstChildOfClass("\072\117\109\097\110\111\105\100")then local t=T.Parent for T,H in pairs(t:GetChildren())do if H:IsA("\080\097\114\116")and(t.Humanoid.Health~=0 and(t.Humanoid.Health>0 and t.Name~=Player.Name))then task.spawn(function()firetouchinterest(H,Brew:getHitbox(),0)wait()firetouchinterest(H,Brew:getHitbox(),1)end)end end end end)end while Brew.isReach==true do(Brew:getHitbox()).Size=Brew.reachMagnitude wait()end end function Brew.undoReach(T)Brew:disableConnection((Brew:getHitbox()):GetPropertyChangedSignal("\083\105\122\101"))Brew:Spoof(Brew:getHitbox(),"\083\105\122\101",Vector3.new(1,.80000001192093,4))Brew.isReach=false if Brew:getHitbox()then(Brew:getHitbox()).Size=Vector3.new(1,.80000001192093,4)end if damageAmplification then damageAmplification:Disconnect()end end function Brew.doSelBox(T)if not(Brew:getHitbox()):FindFirstChildOfClass("\083\101\108\101\099\116\105\111\110\066\111\120")then Brew.selBox=true local T=Instance.new("\083\101\108\101\099\116\105\111\110\066\111\120",Brew:getHitbox())T.Adornee=Brew:getHitbox()T.LineThickness=.01 while Brew.selBox==true do T.Color3=Brew.selBoxColor wait()end end end function Brew.undoSelBox(T)if Brew:getHitbox()and(Brew:getHitbox()):FindFirstChildOfClass("\083\101\108\101\099\116\105\111\110\066\111\120")then Brew.selBox=false wait(.15);((Brew:getHitbox()):FindFirstChildOfClass("\083\101\108\101\099\116\105\111\110\066\111\120")):Destroy()end end function Brew.Patch(T)local t=Instance.new("\083\101\097\116")Brew:Spoof(t,"\080\097\114\101\110\116",nil)local H=Instance.new("\087\101\108\100")Brew:Spoof(H,"\080\097\114\101\110\116",nil)t.Transparency=1 t.CanCollide=false wait(.2)Player.Character.HumanoidRootPart.Anchored=true t.Parent=workspace t.CFrame=Player.Character.HumanoidRootPart.CFrame t.Anchored=false H.Parent=t H.Part0=t H.Part1=Player.Character.HumanoidRootPart Player.Character.HumanoidRootPart.Anchored=false t.CFrame=Player.Character.HumanoidRootPart.CFrame end print("----------------------------------\n\124 \095\095 \041 \095 \095\095 \095\095\095\095\095      \095\095\n\124  \095 \092\124 \'\095\095\047 \095 \092 \092 \047\092 \047 \047\n\124 \124\095\041 \124 \124 \124  \095\095\047\092 \086  \086 \047\n\124\095\095\095\095\047\124\095\124  \092\095\095\095\124 \092\095\047\092\095\047    \064\100\101\120\052\116\119 - \098\108\101\104\n                           \064\115\101\098\118\115\115\099\099 - \115\117\112 \115\107\105\100\115\n----------------------------------------------")Brew:disableConnection((game:GetService("\083\099\114\105\112\116\067\111\110\116\101\120\116")).Error)local T=(loadstring(game:HttpGet("\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\100\097\119\105\100-\115\099\114\105\112\116\115\047\085\073-\076\105\098\115\047\109\097\105\110\047\086\097\112\101\046\116\120\116")))()local t=T:Window("\066\114\101\119 \040"..(game.PlaceId.."\041"),Color3.fromRGB(0,0,0),Enum.KeyCode.RightControl)local H=(game:GetService("\067\111\114\101\071\117\105")):WaitForChild("\117\105");(H.Main:WaitForChild("\084\105\116\108\101")).Text="\066\114\101\119 - \087\097\105\116\105\110\103 \070\111\114 \083\119\111\114\100 \040"..(game.PlaceId.."\041")Sword=Brew:WaitForChildOfClass({Player.Character;Player.Backpack},"\084\111\111\108");(H.Main:WaitForChild("\084\105\116\108\101")).Text="\066\114\101\119 \040"..(game.PlaceId.."\041")local F=t:Tab("\072\111\109\101")local Y=t:Tab("\077\111\100\115")local p=t:Tab("\067\104\097\114\097\099\116\101\114")local h=t:Tab("\065\112\112\101\097\114\097\110\099\101")if not identifyexecutor()=="\070\108\117\120\117\115"then T:Notification("\087\097\114\110\105\110\103","\089\111\117 \097\114\101 \117\115\105\110\103 \097 \098\097\100 \101\120\101\099\117\116\111\114\059 \101\120\112\101\099\116 \102\101\097\116\117\114\101\115 \116\111 \098\101 \100\105\115\097\098\108\101\100 \038 \101\120\112\101\099\116 \102\111\114 \121\111\117 \116\111 \098\101 \100\101\116\101\099\116\101\100\046","\079\10