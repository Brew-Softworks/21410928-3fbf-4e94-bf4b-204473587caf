.reachMagnitude=Vector3.new(tonumber(T),tonumber(T),tonumber(T))end)Y:Dropdown("\082\101\097\099\104 \077\101\116\104\111\100",{"\083\119\111\114\100 \083\112\111\111\102\105\110\103"},function(T)Brew.curReach=T end)Y:Toggle("\083\101\108\101\099\116\105\111\110\066\111\120",false,function(T)if T==true then Brew:doSelBox()Brew.selBox=true elseif T==false then Brew:undoSelBox()Brew.selBox=false end end)Y:Colorpicker("\083\101\108\101\099\116\105\111\110\066\111\120 \067\111\108\111\114",Color3.fromRGB(0,0,0),function(T)Brew.selBoxColor=T end)Y:Toggle("\065\117\116\111\099\108\105\099\107",false,function(T)if T==true then Brew.Autoclick=true while Brew.Autoclick do if(Brew:getSword()).Parent==Player.Character then(Brew:getSword()):Activate()end wait()end elseif T==false then Brew.Autoclick=false end end)p:Toggle("\083\112\105\110",false,function(T)if T==true then Brew.Spin=true if not(Player.Character:FindFirstChild("\072\117\109\097\110\111\105\100\082\111\111\116\080\097\114\116")):FindFirstChildOfClass("\066\111\100\121\065\110\103\117\108\097\114\086\101\108\111\099\105\116\121")then local T=Instance.new("\066\111\100\121\065\110\103\117\108\097\114\086\101\108\111\099\105\116\121",Player.Character:FindFirstChild("\072\117\109\097\110\111\105\100\082\111\111\116\080\097\114\116"))T.AngularVelocity=Vector3.new(0,75,0)T.MaxTorque=Vector3.new(0,9000000000,0)T.P=1250 end elseif T==false then Brew.Spin=false if(Player.Character:FindFirstChild("\072\117\109\097\110\111\105\100\082\111\111\116\080\097\114\116")):FindFirstChildOfClass("\066\111\100\121\065\110\103\117\108\097\114\086\101\108\111\099\105\116\121")then((Player.Character:FindFirstChild("\072\117\109\097\110\111\105\100\082\111\111\116\080\097\114\116")):FindFirstChildOfClass("\066\111\100\121\065\110\103\117\108\097\114\086\101\108\111\099\105\116\121")):Destroy()end end end)p:Textbox("\083\112\101\101\100",true,function(T)Brew:Spoof(Player.Character:WaitForChild("\072\117\109\097\110\111\105\100"),"\087\097\108\107\083\112\101\101\100",16);(Player.Character:WaitForChild("\072\117\109\097\110\111\105\100")).WalkSpeed=tonumber(T)Brew.cWalkspeed=tonumber(T)end)p:Textbox("\074\117\109\112\112\111\119\101\114",true,function(T)Brew:Spoof(Player.Character:WaitForChild("\072\117\109\097\110\111\105\100"),"\074\117\109\112\080\111\119\101\114",50);(Player.Character:WaitForChild("\072\117\109\097\110\111\105\100")).JumpPower=tonumber(T)Brew.cJumppower=tonumber(T)end)h:Colorpicker("\065\099\099\101\110\116 \067\111\108\111\114",Color3.fromRGB(0,0,0),function(t)T:ChangePresetColor(Color3.fromRGB(t.R*255,t.G*255,t.B*255))end)h:Bind("\084\111\103\103\108\101 \085\073",Enum.KeyCode.LeftControl,function()if H.Enabled then H.Enabled=false elseif H.Enabled==false then H.Enabled=true end end)
